<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-override location="classpath:db/db.cfg"/>

    <bean id="placeholderOverrideExample" class="am.ak.spring.data.placeholder.PlaceholderExample">
        <property name="url" value="newTestUrl"/>
        <property name="username" value="newTestUserName"/>
        <property name="password" value="newTestPassword"/>
    </bean>

</beans>